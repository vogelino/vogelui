{
	"name": "vogelui",
	"version": "1.0.2",
	"description": "A collection all-purpose UI elements for building responsive websites",
	"main": "lib/index.js",
	"browser": "lib/index.umd.js",
	"module": "lib/index.es.js",
	"engines": {
		"node": "v12.14.1"
	},
	"files": [
		"/lib"
	],
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook",
		"build": "rollup -c",
		"deploy-storybook": "npm run build-storybook && cd storybook-static && now -A ../now.json --prod",
		"prepublishOnly": "rm -rf lib && npm run build",
		"postbuild": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz",
		"format": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|css|md)\"",
		"lint": "eslint . --ext .js,.jsx,.ts,.tsx"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/vogelino/vogelui.git"
	},
	"keywords": [
		"ui",
		"library",
		"react",
		"responsive",
		"websites",
		"material-ui"
	],
	"author": "Lucas Vogel <contact@vogelino.com> (http://vogelino.com/)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/vogelino/vogelui/issues"
	},
	"homepage": "https://github.com/vogelino/vogelui#readme",
	"devDependencies": {
		"@emotion/core": "^10.0.27",
		"@emotion/styled-base": "^10.0.27",
		"@fortawesome/fontawesome-svg-core": "^1.2.26",
		"@fortawesome/free-solid-svg-icons": "^5.12.0",
		"@fortawesome/react-fontawesome": "^0.1.8",
		"@mdx-js/react": "^1.5.2",
		"@storybook/addon-actions": "^5.3.1",
		"@storybook/addon-links": "^5.3.1",
		"@storybook/addon-viewport": "^5.3.1",
		"@storybook/addons": "^5.3.1",
		"@storybook/react": "^5.3.1",
		"@types/material-ui": "^0.21.7",
		"@types/node": "^12.12.14",
		"@types/react": "^16.9.13",
		"@types/styled-components": "^4.4.0",
		"@types/webpack": "^4.41.0",
		"babel-loader": "^8.0.6",
		"babel-plugin-module-resolver": "^4.0.0",
		"babel-plugin-styled-components": "^1.10.6",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.9.0",
		"eslint-config-react": "^1.1.7",
		"eslint-plugin-json": "^2.0.1",
		"eslint-plugin-prettier": "^3.1.2",
		"eslint-plugin-react": "^7.17.0",
		"eslint-plugin-react-hooks": "^2.3.0",
		"husky": "^4.0.7",
		"lint-staged": "^9.5.0",
		"polished": "^3.4.2",
		"prettier": "^1.19.1",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"react-ripples": "^2.2.1",
		"rollup": "^1.29.0",
		"rollup-plugin-babel": "^4.3.3",
		"rollup-plugin-commonjs": "^10.1.0",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-plugin-peer-deps-external": "^2.2.0",
		"rollup-plugin-terser": "^5.2.0",
		"rollup-plugin-uglify": "^6.0.4",
		"theme-ui": "^0.2.52",
		"ts-loader": "^6.2.1"
	},
	"peerDependencies": {
		"@emotion/core": "^10.0.22",
		"@fortawesome/fontawesome-svg-core": "^1.2.26",
		"@fortawesome/free-solid-svg-icons": "^5.12.0",
		"@fortawesome/react-fontawesome": "^0.1.8",
		"@mdx-js/react": "^1.5.2",
		"prop-types": "^15.7.2",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"styled-components": "^4.4.1",
		"theme-ui": "^0.2.52"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run lint && npm run format"
		}
	},
	"lint-staged": {
		"*.+(js|jsx|ts|tsx)": [
			"eslint --fix",
			"git add"
		],
		"*.+(json|css|md)": [
			"prettier --write",
			"git add"
		]
	},
	"dependencies": {
		"babel-eslint": "^10.0.3",
		"react-select": "^3.0.8"
	}
}
